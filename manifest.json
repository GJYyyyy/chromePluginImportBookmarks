{
    "name": "chromePluginImportBookmarks",
    "version": "1.0",
    "description": "Better Bookmark Import Tool for Chrome Browser",
    "manifest_version": 2,
    "permissions": [
        "bookmarks"
    ],
    "page_action": {
        "default_popup": "src/popup/index.html",
        "default_icon": {
            "16": "images/logo.png",
            "32": "images/logo.png",
            "48": "images/logo.png",
            "128": "images/logo.png"
        }
    }
}